# - Config file for the X3D package
# It defines the following variables
#  X3D_INCLUDE_DIRS - include directories for X3D
#  X3D_LIBRARIES    - libraries to link against
#  X3D_EXECUTABLE   - the bar executable

# Compute paths
get_filename_component(X3D_CMAKE_DIR "${CMAKE_CURRENT_LIST_FILE}" PATH)

list(APPEND CMAKE_MODULE_PATH ${X3D_CMAKE_DIR})

# Our library dependencies (contains definitions for IMPORTED targets)
if(NOT TARGET X3DTargets)
  include("${X3D_CMAKE_DIR}/X3DTargets.cmake")
endif()